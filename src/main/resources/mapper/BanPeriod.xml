<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BanPeriodMapper">
	
	<resultMap id="banPeriodSelectMap" type="banPeriod">
		<result property="report_number" 			column="report_number" 	jdbcType="INT"/>
		<result property="respondent_id"			column="id"				jdbcType="VARCHAR" />
		<result property="respondent_nickname" 		column="nickname" 		jdbcType="VARCHAR" />
		<result property="respondent_store_name" 	column="store_name" 	jdbcType="VARCHAR" />
		<result property="status_number" 			column="status_number" 	jdbcType="INT" />
		<result property="ban_period_number" 		column="period_number" 	jdbcType="INT" />
		<result property="ban_end_date" 			column="end_date" 		jdbcType="DATE" />
	</resultMap>


  


</mapper>